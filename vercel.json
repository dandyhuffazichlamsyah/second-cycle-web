{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|fonts|storage|build)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "SecondCycle",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://second-cycle-web.vercel.app",
        "DB_CONNECTION": "sqlite",
        "SESSION_DRIVER": "cookie",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr"
    }
}